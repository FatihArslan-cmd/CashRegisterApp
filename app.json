{
  "expo": {
    "name": "project",
    "slug": "project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "USE_BIOMETRIC"
        ]
      },
      "package": "com.fatiharslan.project"
    },
    "web": {
      "favicon": "./assets/image/adaptive-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e75a308b-7702-479d-a615-972a6a6ce8b5"
      }
    }
  }
}
